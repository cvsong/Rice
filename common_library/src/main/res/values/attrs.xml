<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定义底部导航栏-->
    <declare-styleable name="CustomBottomNavigationView">
        <attr name="top_icon" format="reference"/>
        <attr name="bottom_text" format="string"/>
    </declare-styleable>


    <!--圆形图像控件-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension"/>
        <attr name="civ_border_color" format="color"/>
        <attr name="civ_border_overlay" format="boolean"/>
        <attr name="civ_fill_color" format="color"/>
    </declare-styleable>



    <!--子View悬浮的ScroolView-->
    <declare-styleable name="StickyScrollView">
        <attr name="stuckShadowHeight" format="dimension" />
        <attr name="stuckShadowDrawable" format="reference" />
    </declare-styleable>


    <!-- 可清空编辑框属性 -->
    <declare-styleable name="EditTextWithDel">
        <attr name="delSrc" format="reference"/>
    </declare-styleable>


    <!--侧滑删除功能的View-->
    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>


    <!--倒计时按钮控件-->
    <declare-styleable name="CountDownButton">
        <attr name="countDownFormat" format="string" />
        <attr name="countDown" format="float" />
        <attr name="countDownInterval" format="float" />
        <attr name="enableCountDown" format="boolean" />
    </declare-styleable>

</resources>